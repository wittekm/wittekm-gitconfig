[alias]
    st=fast-status
    br=branch
    co=fast-checkout
    cm=fast-commit
    dt=difftool
    dtu=difftool @{upstream}
    cane=commit --amend --no-edit

    # branch ordered by date, or, 'bro'
    bro=for-each-ref --sort=committerdate refs/heads/ --format='%(committerdate:short) %(refname:short)'
    cou=checkout @{upstream} -- 
